{
	"$schema": "vscode://schemas/color-theme",
	"name": "Relaxed Dark",
	"type": "dark",	
	"colors": {
		// --------------- WELCOME ---------------
		"walkThrough.embeddedEditorBackground": "#242D34ff", // MARKDOWN CODE BG
		"welcomePage.buttonBackground": "#1F1E1Fff",
		"welcomePage.buttonHoverBackground": "#353435ff",

		// --------------- MAIN APP ---------------
		// ----- Title bar -----
		"titleBar.activeBackground": "#121317ff", 
		"titleBar.inactiveBackground": "#1d1f25ff",
		"titleBar.activeForeground": "#C0B1AFff",
		"titleBar.inactiveForeground": "#3B3636ff",
		
		// ----- Menu (File, Edit, Selection , etc.) -----
		"menu.background": "#121317ff",
		
		// ----- Dropdowns (Like the ones in settings) -----
		"dropdown.background": "#121317ff",
		"dropdown.border": "#1F1E1Fff",
		"dropdown.foreground": "#C0B1AFff",
		
		// ----- Activity bar -----
	  	"activityBar.background": "#121317ff",
	  	"activityBar.border": "#1F1E1Fff",
	  	"activityBar.foreground": "#C0B1AFff", // Changes the color of the icons
		"activityBarBadge.background": "#1F1E1Fff", 
		"activityBarBadge.foreground": "#C0B1AFff",

		// ----- Sidebar ----- 
		"sideBar.background": "#121317ff",
		"sideBar.border": "#1F1E1Fff",
		"sideBar.foreground": "#9C8481ff", // This is the default sidebar file text color
		"sideBarSectionHeader.background": "#1F1E1Fff",
		"sideBarSectionHeader.foreground": "#C0B1AFff",
		"sideBarTitle.foreground": "#C0B1AFff",

		// ----- -----
		"list.activeSelectionBackground": "#353435ff",
		"list.activeSelectionForeground": "#C0B1AFff",
		"list.hoverBackground": "#353435ff",
		"list.hoverForeground": "#C0B1AFff", 
		"list.inactiveSelectionBackground": "#353435ff", 
		"list.inactiveSelectionForeground": "#C0B1AFff",
